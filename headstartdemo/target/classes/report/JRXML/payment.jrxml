<?xml version="1.0" encoding="UTF-8"?>
<jasperReport bottomMargin="20" columnWidth="625" language="groovy"
    leftMargin="10" name="report" pageHeight="842" pageWidth="725"
    rightMargin="10" topMargin="20"
    xmlns="http://jasperreports.sourceforge.net/jasperreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd">
    <style fontName="Times New Roman" fontSize="50" forecolor="#FFFFFF"
        isBold="false" name="Title" pdfFontName="Times-Bold"/>
    <style fontName="Times New Roman" fontSize="18" forecolor="#CCCCCC"
        isBold="false" name="SubTitle" pdfFontName="Times-Roman"/>
    <style fontName="Times New Roman" fontSize="14" forecolor="#666666"
        isBold="true" name="Column header" pdfFontName="Times-Roman"/>
    <style fontName="Times New Roman" mode="Transparent" name="Detail" pdfFontName="Times-Roman"/>
    <style fontName="Times New Roman" mode="Transparent" name="Row" pdfFontName="Times-Roman">
        <conditionalStyle>
            <conditionExpression><![CDATA[$V{REPORT_COUNT}%2 == 0]]></conditionExpression>
            <style backcolor="#F0EFEF" mode="Opaque"/>
        </conditionalStyle>
    </style>
    <queryString language="SQL"><![CDATA[select * from payment]]></queryString>
    <field class="INT UNSIGNED" name="customerid"/>
    <field class="java.lang.String" name="checkNumber"/>
    <field class="java.sql.Timestamp" name="payment_date"/>
    <field class="java.lang.Double" name="amount"/>
    <field class="java.math.BigInteger" name="id"/>
    <background>
        <band splitType="Stretch"/>
    </background>
    <title>
        <band height="100" splitType="Stretch">
            <staticText>
                <reportElement forecolor="#FF3333" height="45"
                    style="Title" width="250" x="5" y="7"/>
                <textElement>
                    <font isBold="false" size="30"/>
                </textElement>
                <text><![CDATA[Report]]></text>
            </staticText>
            <staticText>
                <reportElement forecolor="#FF3333" height="25"
                    style="Title" width="200" x="400" y="5"/>
                <textElement>
                    <font isBold="false" size="18"/>
                </textElement>
                <text><![CDATA[Application Name: headstartdemo]]></text>
            </staticText>
            <staticText>
                <reportElement forecolor="#000000" height="25"
                    style="Title" width="200" x="400" y="35"/>
                <textElement>
                    <font isBold="false" size="12"/>
                </textElement>
                <text><![CDATA[Generated By: Team]]></text>
            </staticText>
        </band>
    </title>
    <pageHeader>
        <band height="40" splitType="Stretch">
            <staticText>
                <reportElement forecolor="#000000" height="25"
                    style="Title" width="500" x="5" y="5"/>
                <textElement>
                    <font isBold="true" size="18"/>
                </textElement>
                <text><![CDATA[The report of the table payment is as bellow:]]></text>
            </staticText>
        </band>
    </pageHeader>
    <columnHeader>
        <band height="26" splitType="Stretch">
            <staticText>
                <reportElement backcolor="#FFCCCC" forecolor="#000000"
                    height="18" mode="Opaque" style="Column header"
                    width="120" x="0" y="0"/>
                <textElement>
                    <font isBold="true" size="15"/>
                </textElement>
                <text><![CDATA[customerid]]></text>
            </staticText>
            <staticText>
                <reportElement backcolor="#FFCCCC" forecolor="#000000"
                    height="18" mode="Opaque" style="Column header"
                    width="120" x="120" y="0"/>
                <textElement>
                    <font isBold="true" size="15"/>
                </textElement>
                <text><![CDATA[checkNumber]]></text>
            </staticText>
            <staticText>
                <reportElement backcolor="#FFCCCC" forecolor="#000000"
                    height="18" mode="Opaque" style="Column header"
                    width="120" x="240" y="0"/>
                <textElement>
                    <font isBold="true" size="15"/>
                </textElement>
                <text><![CDATA[payment_date]]></text>
            </staticText>
            <staticText>
                <reportElement backcolor="#FFCCCC" forecolor="#000000"
                    height="18" mode="Opaque" style="Column header"
                    width="120" x="360" y="0"/>
                <textElement>
                    <font isBold="true" size="15"/>
                </textElement>
                <text><![CDATA[amount]]></text>
            </staticText>
            <staticText>
                <reportElement backcolor="#FFCCCC" forecolor="#000000"
                    height="18" mode="Opaque" style="Column header"
                    width="120" x="480" y="0"/>
                <textElement>
                    <font isBold="true" size="15"/>
                </textElement>
                <text><![CDATA[id]]></text>
            </staticText>
        </band>
    </columnHeader>
    <detail>
        <band height="18" splitType="Stretch">
            <frame>
                <reportElement height="18" mode="Opaque" style="Row"
                    width="600" x="0" y="0"/>
                <textField isStretchWithOverflow="true">
                    <reportElement backcolor="#CCFFFF" height="18"
                        mode="Transparent" positionType="Float"
                        style="Detail" width="120" x="0" y="0"/>
                    <textElement textAlignment="Left" verticalAlignment="Top">
                        <font isBold="true" isStrikeThrough="false" size="14"/>
                    </textElement>
                    <textFieldExpression><![CDATA[$F{customerid}]]></textFieldExpression>
                </textField>
                <textField isStretchWithOverflow="true">
                    <reportElement backcolor="#CCFFFF" height="18"
                        mode="Transparent" positionType="Float"
                        style="Detail" width="120" x="120" y="0"/>
                    <textElement textAlignment="Left" verticalAlignment="Top">
                        <font isBold="true" isStrikeThrough="false" size="14"/>
                    </textElement>
                    <textFieldExpression><![CDATA[$F{checkNumber}]]></textFieldExpression>
                </textField>
                <textField isStretchWithOverflow="true">
                    <reportElement backcolor="#CCFFFF" height="18"
                        mode="Transparent" positionType="Float"
                        style="Detail" width="120" x="240" y="0"/>
                    <textElement textAlignment="Left" verticalAlignment="Top">
                        <font isBold="true" isStrikeThrough="false" size="14"/>
                    </textElement>
                    <textFieldExpression><![CDATA[$F{payment_date}]]></textFieldExpression>
                </textField>
                <textField isStretchWithOverflow="true">
                    <reportElement backcolor="#CCFFFF" height="18"
                        mode="Transparent" positionType="Float"
                        style="Detail" width="120" x="360" y="0"/>
                    <textElement textAlignment="Left" verticalAlignment="Top">
                        <font isBold="true" isStrikeThrough="false" size="14"/>
                    </textElement>
                    <textFieldExpression><![CDATA[$F{amount}]]></textFieldExpression>
                </textField>
                <textField isStretchWithOverflow="true">
                    <reportElement backcolor="#CCFFFF" height="18"
                        mode="Transparent" positionType="Float"
                        style="Detail" width="120" x="480" y="0"/>
                    <textElement textAlignment="Left" verticalAlignment="Top">
                        <font isBold="true" isStrikeThrough="false" size="14"/>
                    </textElement>
                    <textFieldExpression><![CDATA[$F{id}]]></textFieldExpression>
                </textField>
            </frame>
        </band>
    </detail>
    <columnFooter>
        <band height="4" splitType="Stretch">
            <line>
                <reportElement height="1"
                    positionType="FixRelativeToBottom" width="600" x="0" y="3"/>
                <graphicElement/>
            </line>
        </band>
    </columnFooter>
    <pageFooter>
        <band height="27" splitType="Stretch">
            <frame>
                <reportElement backcolor="#000000" forecolor="#D0B48E"
                    height="20" mode="Opaque" width="600" x="0" y="1"/>
                <textField evaluationTime="Report">
                    <reportElement forecolor="#FFFFFF" height="20"
                        style="Column header" width="40" x="520" y="0"/>
                    <textElement verticalAlignment="Middle"/>
                    <textFieldExpression><![CDATA[" "+ $V{PAGE_NUMBER}]]></textFieldExpression>
                </textField>
                <textField>
                    <reportElement forecolor="#FFFFFF" height="20"
                        style="Column header" width="80" x="440" y="0"/>
                    <textElement textAlignment="Right" verticalAlignment="Middle">
                        <font isBold="true" size="10"/>
                    </textElement>
                    <textFieldExpression><![CDATA["Page "+$V{PAGE_NUMBER}+" of"]]></textFieldExpression>
                </textField>
            </frame>
        </band>
    </pageFooter>
    <summary>
        <band height="0" splitType="Stretch"/>
    </summary>
</jasperReport>
